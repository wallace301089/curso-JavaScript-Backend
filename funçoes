// funções
// existe tres tipos de funçoes



// Declarando a função 
function imprimeTexto(texto) {
    console.log(texto)

}
imprimeTexto("oi")
imprimeTexto("outro texto")
imprimeTexto(soma())
// funçoes queu nao recebem parametro
function soma() {
    return 2 + 2;
} //console.log(soma())